{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "^@backend/(.*)$": "<rootDir>/../src/$1"
  },
  "setupFilesAfterEnv": [
    "./setup-each-test.ts"
  ]
}
